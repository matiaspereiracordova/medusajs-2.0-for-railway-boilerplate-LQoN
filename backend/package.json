{
  "name": "medusa-2.0-boilerplate-backend",
  "version": "0.0.2",
  "description": "A Prebaked Medusa 2.0 backend",
  "author": "Rasmus Puls",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build && node src/scripts/postBuild.js",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "seed:force": "FORCE_SEED=true medusa exec ./src/scripts/seed.ts",
    "seed:verify": "node verify-seed-status.js",
    "seed:reset": "node reset-seed-marker.js",
    "ib": "init-backend",
    "start": "init-backend && cd .medusa/server && medusa start --verbose",
    "start:with-seed": "init-backend && cd .medusa/server && medusa exec ../../src/scripts/seed.ts && medusa start --verbose",
    "railway:post-deploy": "node railway-post-deploy.js",
    "railway:test-seed": "node test-railway-seed.js",
    "railway:seed": "node execute-railway-seed.js",
    "railway:check": "node check-railway-products.js",
    "dev": "medusa develop",
    "email:dev": "email dev --dir=./src/modules/email-notifications/templates --port=3002",
    "cleanup-odoo-duplicates": "medusa exec ./src/scripts/cleanup-odoo-duplicates.ts",
    "identify-odoo-duplicates": "medusa exec ./src/scripts/cleanup-odoo-duplicates.ts identify",
    "test-odoo-cleanup": "medusa exec ./src/scripts/test-odoo-cleanup.ts",
    "cleanup-odoo-standalone": "ts-node ./src/scripts/cleanup-odoo-duplicates-standalone.ts",
    "identify-odoo-standalone": "ts-node ./src/scripts/cleanup-odoo-duplicates-standalone.ts identify",
    "cleanup-odoo-simple": "ts-node ./src/scripts/cleanup-odoo-duplicates-simple.ts",
    "identify-odoo-simple": "ts-node ./src/scripts/cleanup-odoo-duplicates-simple.ts identify",
    "create-dog-products": "medusa exec ./src/scripts/create-dog-products.ts"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.10.2",
    "@medusajs/cli": "2.10.2",
    "@medusajs/dashboard": "2.10.2",
    "@medusajs/draft-order": "^2.10.2",
    "@medusajs/framework": "2.10.2",
    "@medusajs/medusa": "2.10.2",
    "@medusajs/notification-sendgrid": "2.10.2",
    "@medusajs/payment-stripe": "2.10.2",
    "@medusajs/workflow-engine-redis": "2.10.2",
    "@medusajs/workflows-sdk": "^2.10.3",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@react-email/components": "^0.0.26",
    "@rokmohar/medusa-plugin-meilisearch": "1.3.5",
    "awilix": "^8.0.1",
    "json-rpc-2.0": "^1.7.1",
    "medusajs-launch-utils": "0.0.18",
    "minio": "^8.0.3",
    "pg": "^8.13.1",
    "resend": "4.0.1",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.10.2",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.37",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.10",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-email": "^3.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11"
  },
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  }
}
